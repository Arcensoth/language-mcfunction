{
  "name": "lanuage-mcfunction",
  "displayName": "language-mcfunction",
  "description": "TextMate language grammar for mcfunction files.",
  "version": "0.0.1",
  "repository": "https://github.com/Arcensoth/language-mcfunction",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b build -v && node build/build.js",
    "debug": "tsc -b build -v && node --nolazy --inspect-brk=9229 build/build.js"
  },
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mcfunction",
        "aliases": [
          "mcfunction"
        ],
        "extensions": [
          "mcfunction"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mcfunction",
        "scopeName": "source.mcfunction",
        "path": "./mcfunction.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "latest",
    "typescript": "latest",
    "@types/js-yaml": "latest",
    "@types/node": "latest"
  }
}
